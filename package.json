{
  "name": "@competec/eslint-config-competec",
  "version": "2.0.0",
  "description": "Competec ESLint Rules",
  "keywords": [
    "eslint",
    "eslintconfig",
    "eslint-config",
    "competec"
  ],
  "files": [
    "src/*"
  ],
  "author": "Raphael Haettich / Competec",
  "main": "src/index.js",
  "peerDependencies": {
    "eslint": "6.x"
  },
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/gitlab": "^4.0.0",
    "eslint": "^6.3.0",
    "husky": "^3.0.9",
    "semantic-git-commit-cli": "^3.5.0",
    "semantic-release": "^15.13.24"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "babel-eslint": "^10.0.3",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1"
  },
  "scripts": {
    "lint": "eslint *.js",
    "lint-fix": "eslint *.js --fix",
    "commit": "BRANCH=$(git rev-parse --abbrev-ref HEAD | grep -oP '.*((?:VOR|DEV|DO|SEARCH|SHOP3)-[0-9]+)' || git rev-parse --abbrev-ref HEAD);sgc -s $(echo $BRANCH | sed -r \"s/\\x1B\\[([0-9]{1,2}(;[0-9]{1,2})?)?[m|K]//g\")",
    "gbc": "git add . && yarn commit"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:competec-opensource/eslint-config-competec.git"
  },
  "bugs": {
    "url": "https://gitlab.com:competec-opensource/eslint-config-competec/issues"
  },
  "homepage": "https://gitlab.com:competec-opensource/eslint-config-competec#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
